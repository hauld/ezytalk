    {"id": 2, "lang": "fr-FR", "mute": false, "vocal": "Mathieu", "text":"Bonojour madame?", "audio":"assets/lesson-1/audio/2.mp3", "timer": 100, "imageUrl":"assets/lesson-1/img/1.jpeg"},
    {"id": 3, "lang": "fr-FR", "mute": true, "vocal": "Lea", "text":"Bonjour, monsieur, je voudrais un croissant au beurre, deux pains au chocolat et une brioche au sucre, s'il vous plaît", "audio":"assets/lesson-1/audio/3.mp3", "timer": 100, "imageUrl":"assets/lesson-1/img/1.jpeg"},
